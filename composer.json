{
    "name": "jgrygierek/batch-entity-import-bundle",
    "type": "symfony-bundle",
    "description": "Importing entities with preview and edit features for Symfony.",
    "keywords": [
        "entity",
        "import",
        "preview",
        "excel",
        "xls",
        "xlsx",
        "csv",
        "ods",
        "symfony",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "JarosÅ‚aw Grygierek",
            "email": "jaroslawgrygierek@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/orm": "^2.7",
        "phpoffice/phpspreadsheet": "^1.10",
        "symfony/doctrine-bridge": "^4.4|^5.0",
        "symfony/form": "^4.4|^5.0",
        "symfony/framework-bundle": "^4.4|^5.0",
        "symfony/property-access": "^4.4|^5.0",
        "symfony/translation": "^4.4|^5.0",
        "symfony/twig-bundle": "^4.4|^5.0",
        "symfony/validator": "^4.4|^5.0",
        "twig/extensions": "^1.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "escapestudios/symfony2-coding-standard": "3.x-dev",
        "knplabs/doctrine-behaviors": "~2.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1.1",
        "phpmd/phpmd": "^2.8",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "~3.5",
        "symfony/browser-kit": "^5.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JG\\BatchEntityImportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JG\\BatchEntityImportBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs src",
        "phpmd": "vendor/bin/phpmd src text cleancode,codesize,controversial,design,naming,unusedcode",
        "tests": "vendor/bin/simple-phpunit",
        "tests-cov": "vendor/bin/simple-phpunit --coverage-clover=coverage.xml"
    }
}
