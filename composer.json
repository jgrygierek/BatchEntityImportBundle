{
    "name": "jgrygierek/batch-entity-import-bundle",
    "type": "symfony-bundle",
    "description": "Importing entities with preview and edit features for Symfony.",
    "keywords": [
        "entity",
        "import",
        "preview",
        "excel",
        "xls",
        "xlsx",
        "csv",
        "ods",
        "symfony",
        "symfony"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "JarosÅ‚aw Grygierek",
            "email": "jaroslawgrygierek@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/orm": "^2.7",
        "doctrine/lexer": "^1.2.1",
        "phpoffice/phpspreadsheet": "^1.22.0",
        "symfony/config": "^4.4.12|^5.1.4|^6.0",
        "symfony/dependency-injection": "^4.4.12|^5.1.9|^6.0",
        "symfony/doctrine-bridge": "^4.4|^5.0|^6.0",
        "symfony/form": "^4.4|^5.0|^6.0",
        "symfony/framework-bundle": "^4.4|^5.0|^6.0",
        "symfony/property-access": "^4.4|^5.0|^6.0",
        "symfony/translation": "^4.4.12|^5.1.4|^6.0",
        "symfony/twig-bundle": "^4.4|^5.0|^6.0",
        "symfony/validator": "^4.4.11|^5.0.11|^6.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "knplabs/doctrine-behaviors": "^2.0.8",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1.1",
        "roave/security-advisories": "dev-latest",
        "symfony/browser-kit": "^5.1|^6.0",
        "symfony/dom-crawler": "^5.1.4|^6.0",
        "symfony/phpunit-bridge": "^5.3.8|^6.0",
        "symfony/security-bundle": "^4.4|^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JG\\BatchEntityImportBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JG\\BatchEntityImportBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "php-cs-fixer": "vendor/bin/php-cs-fixer fix -v --dry-run --diff",
        "php-cs-fixer-fix": "vendor/bin/php-cs-fixer fix -v --diff",
        "tests": "vendor/bin/simple-phpunit",
        "tests-cov": "vendor/bin/simple-phpunit --coverage-clover=coverage.xml"
    }
}
